  "main": "./dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "./dist/es2015/index.js",
  "repository": "https://github.com/w11k/rx-utils",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "chai": "4.1.2",
    "del-cli": "1.1.0",
    "gh-pages": "^1.2.0",
    "mocha": "5.0.0",
    "source-map-support": "0.5.3",
    "ts-node": "4.1.0",
    "tslint": "^5.9.1",
    "typedoc": "0.12.0",
    "typescript": "^3.1.6",
    "typings": "^2.1.1"
  },
  "peerDependencies": {
    "rxjs": "^6.0.0"
  },
  "scripts": {
    "clean": "del-cli dist",
    "build": "npm run clean ; tsc -p tsconfig.json && tsc -p tsconfig.dist.es2015.json",
    "prepublishOnly": "npm test && npm run build",
    "postpublish": "npm run typedoc_deploy && git push",
    "test": "mocha --opts spec/mocha.opts src/**/*test.ts",
    "typedoc": "typedoc --out doc/ src && echo \"\" > doc/.nojekyll",
    "typedoc_deploy": "npm run typedoc && gh-pages -t --dotfiles -d doc"
  }
}
